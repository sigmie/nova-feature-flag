{
    "name": "sigmie/nova-feature-flags",
    "description": "A Laravel Nova tool for Managing Feature flags.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "ylsideas/feature-flags": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Sigmie\\NovaFeatureFlags\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sigmie\\NovaFeatureFlags\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}